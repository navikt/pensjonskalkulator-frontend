{
  "name": "pensjonskalkulator-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "prebuild": "npm run eslint:check && npm run prettier:check && npm run stylelint:check",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:update": "vitest run -u --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "cy:open": "cypress open",
    "cy:run": "ELECTRON_EXTRA_LAUNCH_ARGS=\"--force-prefers-reduced-motion\" cypress run",
    "cy:test": "start-server-and-test preview http://localhost:4173/pensjon/kalkulator cy:run",
    "eslint:check": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "stylelint:check": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "prettier:check": "prettier --check '**/*.{cjs,js,jsx,ts,tsx,json}'",
    "prettier:fix": "prettier --write '**/*.{cjs,js,jsx,ts,tsx,json}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formatjs/intl-datetimeformat": "^6.10.0",
    "@formatjs/intl-numberformat": "^8.7.0",
    "@grafana/faro-web-sdk": "^1.1.3",
    "@navikt/aksel-icons": "^4.11.2",
    "@navikt/ds-css": "^4.11.2",
    "@navikt/ds-icons": "^3.4.3",
    "@navikt/ds-react": "^4.11.2",
    "@navikt/ds-tokens": "^4.11.2",
    "@reduxjs/toolkit": "^1.9.5",
    "clsx": "^1.2.1",
    "highcharts": "^11.1.0",
    "highcharts-react-official": "^3.2.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@navikt/nav-dekoratoren-moduler": "^2.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vitest/ui": "^0.28.5",
    "axe-core": "^4.6.3",
    "cross-fetch": "^3.1.5",
    "cypress": "^12.7.0",
    "cypress-axe": "^1.4.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-typescript": "^1.0.10",
    "eslint-plugin-import": "^2.27.5",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^14.0.0",
    "msw": "^1.2.0",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.28",
    "postcss-modules-extract-imports": "^3.0.0",
    "postcss-modules-local-by-default": "^4.0.3",
    "postcss-modules-scope": "^3.0.0",
    "postcss-modules-values": "^4.0.0",
    "prettier": "^2.8.4",
    "react-intl": "^6.4.4",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.58.0",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.1.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "timers": "^0.1.1",
    "typescript": "^4.9.3",
    "vite": "^4.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-sass-dts": "^1.3.9",
    "vite-plugin-stylelint": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "**/*.{css,scss}": [
      "npm run stylelint:check"
    ],
    "**/*.{cjs,js,jsx,ts,tsx}": [
      "npm run prettier:check",
      "npm run eslint:check"
    ]
  }
}
