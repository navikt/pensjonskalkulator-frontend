{
  "name": "pensjonskalkulator-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "prebuild": "npm run stylelint:check && npm run eslint:check && npm run prettier:check",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "eslint:check": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "stylelint:check": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "prettier:check": "prettier --check '**/*.{cjs,js,jsx,ts,tsx,.css,.scss,.json}'",
    "prettier:fix": "prettier --write '**/*.{cjs,js,jsx,ts,tsx,.css,.scss,.json}'"
  },
  "dependencies": {
    "@navikt/ds-css": "~2.1.7",
    "@navikt/ds-react": "~2.1.7",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-typescript": "^1.0.10",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.4",
    "sass": "^1.58.0",
    "stylelint": "^15.1.0",
    "stylelint-config-standard-scss": "^7.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^4.2.0"
  }
}
