{
  "name": "pensjonskalkulator-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:production": "vite --mode production",
    "sanity-dev": "sanity dev -- --port 3333",
    "sanity-start": "sanity start dist-sanity/",
    "sanity-build": "sanity build dist-sanity/ --yes",
    "fetch-land-liste": "node --experimental-fetch ./scripts/FetchLandListe.js",
    "fetch-dev-types": "openapi-typescript https://pensjonskalkulator-backend.intern.dev.nav.no/v3/api-docs/current -o ./src/types/schema.d.ts",
    "postfetch-dev-types": "npm run format:types",
    "format:types": "prettier --write ./src/types/schema.d.ts && eslint ./src/types/schema.d.ts --fix",
    "prebuild": "npm run fetch-land-liste && npm run eslint:check && npm run prettier:check && npm run stylelint:check",
    "build": "tsc && vite build --mode staging",
    "build:server": "tsc -p tsconfig.server.json",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest run --pool=forks --coverage",
    "test:update": "vitest run --pool=forks -u",
    "test:watch": "vitest --pool=forks",
    "test:ui": "vitest --pool=forks --ui",
    "cy:open": "cypress open",
    "cy:test": "start-server-and-test preview http://localhost:4173/pensjon/kalkulator 'ELECTRON_EXTRA_LAUNCH_ARGS=\"--force-prefers-reduced-motion\" cypress run'",
    "cy:test:production": "start-server-and-test preview http://localhost:4173/pensjon/kalkulator 'ELECTRON_EXTRA_LAUNCH_ARGS=\"--force-prefers-reduced-motion\" cypress run --env DECORATOR_URL=https://www.nav.no/dekoratoren'",
    "eslint:check": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "stylelint:check": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "prettier:check": "prettier --check '**/*.{cjs,js,jsx,ts,tsx,json}'",
    "prettier:fix": "prettier --write '**/*.{cjs,js,jsx,ts,tsx,json}'",
    "prepare": "husky"
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.5.3",
    "@navikt/oasis": "^3.7.0",
    "@portabletext/react": "^3.2.0",
    "@sanity/vision": "^3.76.1",
    "express": "^4.21.2",
    "express-prom-bundle": "^8.0.0",
    "http-proxy-middleware": "^3.0.3",
    "prom-client": "^15.1.3",
    "sanity": "^3.77.1",
    "styled-components": "^6.1.15",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.26.0",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.20.0",
    "@formatjs/ecma402-abstract": "^2.3.1",
    "@formatjs/intl-datetimeformat": "^6.17.3",
    "@formatjs/intl-numberformat": "^8.15.3",
    "@grafana/faro-web-sdk": "^1.14.1",
    "@grafana/faro-web-tracing": "^1.14.0",
    "@navikt/aksel-icons": "^7.14.2",
    "@navikt/ds-css": "^7.14.2",
    "@navikt/ds-react": "^7.14.2",
    "@navikt/nav-dekoratoren-moduler": "^3.2.2",
    "@reduxjs/toolkit": "^2.6.0",
    "@sanity/client": "^6.28.0",
    "@sanity/document-internationalization": "^3.3.1",
    "@sanity/eslint-config-studio": "^5.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.6",
    "axe-core": "^4.10.2",
    "clsx": "^2.1.1",
    "cypress": "^14.1.0",
    "cypress-axe": "^1.6.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "highcharts": "^12.1.2",
    "highcharts-react-official": "^3.2.1",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "msw": "^2.7.3",
    "openapi-typescript": "^7.6.1",
    "postcss": "^8.5.3",
    "postcss-modules-extract-imports": "^3.1.0",
    "postcss-modules-local-by-default": "^4.2.0",
    "postcss-modules-scope": "^3.2.1",
    "postcss-modules-values": "^4.0.0",
    "prettier": "^3.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intl": "7.1.6",
    "react-redux": "^9.2.0",
    "react-router": "^7.2.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.85.1",
    "start-server-and-test": "^2.0.10",
    "stylelint": "^16.14.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "timers": "^0.1.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-sass-dts": "^1.3.29",
    "vite-plugin-stylelint": "^6.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.6"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "**/*.{css,scss}": [
      "stylelint"
    ],
    "**/*.{cjs,js,jsx,ts,tsx}": [
      "prettier",
      "eslint"
    ]
  }
}
