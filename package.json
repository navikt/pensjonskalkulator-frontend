{
  "name": "pensjonskalkulator-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:q2": "concurrently \"NODE_ENV=production npm run start\" \"nodemon --watch server/ --exec 'npm run build:server && NODE_ENV=development node dist/server/server.js'\"",
    "start:production": "vite --mode production",
    "sanity-dev": "sanity dev -- --port 3333",
    "sanity-start": "sanity start dist-sanity/",
    "sanity-build": "sanity build dist-sanity/ --yes",
    "sanity-typegen": "npx sanity schema extract --enforce-required-fields --workspace pensjonskalkulator-frontend-development-workspace --path schemaTypes/sanity-schema.json && npx sanity typegen generate --config-path schemaTypes/sanity-typegen.json",
    "fetch-land-liste": "node --experimental-fetch ./scripts/FetchLandListe.js",
    "fetch-sanity-data": "node --experimental-fetch ./scripts/FetchSanityData.js",
    "fetch-dev-types": "openapi-typescript https://pensjonskalkulator-backend.intern.dev.nav.no/v3/api-docs/current -o ./src/types/schema.d.ts",
    "postfetch-dev-types": "npm run format:types",
    "format:types": "prettier --write ./src/types/schema.d.ts && eslint ./src/types/schema.d.ts --fix",
    "prebuild": "npm run fetch-land-liste && npm run fetch-sanity-data && npm run eslint:check && npm run prettier:check && npm run stylelint:check",
    "build": "tsc && vite build --mode staging",
    "build:server": "tsc -p tsconfig.server.json",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest run --pool=forks --coverage",
    "test:update": "vitest run --pool=forks -u",
    "test:watch": "vitest --pool=forks",
    "test:ui": "vitest --pool=forks --ui",
    "typecheck": "tsc --noEmit --pretty",
    "cy:open": "cypress open",
    "cy:test": "start-server-and-test preview http://localhost:4173/pensjon/kalkulator 'ELECTRON_EXTRA_LAUNCH_ARGS=\"--force-prefers-reduced-motion\" cypress run --headed'",
    "cy:test:production": "start-server-and-test preview http://localhost:4173/pensjon/kalkulator 'ELECTRON_EXTRA_LAUNCH_ARGS=\"--force-prefers-reduced-motion\" cypress run --env DECORATOR_URL=https://www.nav.no/dekoratoren'",
    "eslint:check": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "stylelint:check": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "prettier:check": "prettier --check '**/*.{cjs,js,jsx,ts,tsx,json}'",
    "prettier:fix": "prettier --write '**/*.{cjs,js,jsx,ts,tsx,json}'",
    "prepare": "husky"
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.5.3",
    "@navikt/oasis": "^3.8.0",
    "@portabletext/react": "^3.2.1",
    "@sanity/icons": "^3.7.4",
    "@sanity/ui": "^2.16.2",
    "@sanity/vision": "^3.95.0",
    "express": "^4.21.2",
    "express-prom-bundle": "^8.0.0",
    "groq": "^3.95.0",
    "http-proxy-middleware": "^3.0.5",
    "prom-client": "^15.1.3",
    "sanity": "^3.95.0",
    "styled-components": "^6.1.19",
    "unleash-client": "^6.6.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.27.1",
    "@eslint/js": "^9.30.1",
    "@formatjs/intl-numberformat": "^8.15.4",
    "@grafana/faro-web-sdk": "^1.18.2",
    "@grafana/faro-web-tracing": "^1.18.2",
    "@navikt/aksel-icons": "^7.24.0",
    "@navikt/ds-css": "^7.24.0",
    "@navikt/ds-react": "^7.24.0",
    "@navikt/nav-dekoratoren-moduler": "^3.3.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@sanity/client": "^7.6.0",
    "@sanity/document-internationalization": "^3.3.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/express": "^5.0.1",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "axe-core": "^4.10.3",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.1",
    "cypress-axe": "^1.6.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "highcharts": "^12.3.0",
    "highcharts-react-official": "^3.2.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.2",
    "nodemon": "^3.1.10",
    "openapi-typescript": "^7.8.0",
    "postcss": "^8.5.6",
    "postcss-modules-extract-imports": "^3.1.0",
    "postcss-modules-local-by-default": "^4.2.0",
    "postcss-modules-scope": "^3.2.1",
    "postcss-modules-values": "^4.0.0",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intl": "7.1.11",
    "react-redux": "^9.2.0",
    "react-router": "7.6.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.89.2",
    "start-server-and-test": "^2.0.12",
    "stylelint": "^16.21.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "timers": "^0.1.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.3.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-sass-dts": "^1.3.31",
    "vite-plugin-stylelint": "^6.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "3.2.4"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "**/*.{css,scss}": [
      "stylelint"
    ],
    "**/*.{cjs,js,jsx,ts,tsx}": [
      "prettier",
      "eslint"
    ]
  }
}
