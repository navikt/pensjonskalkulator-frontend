{
  "name": "pensjonskalkulator-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:production": "vite --mode production",
    "fetch-dev-types": "openapi-typescript https://pensjonskalkulator-backend.intern.dev.nav.no/v3/api-docs/api -o ./src/types/schema.d.ts",
    "postfetch-dev-types": "npm run format:types",
    "format:types": "prettier --write ./src/types/schema.d.ts && eslint ./src/types/schema.d.ts --fix",
    "prebuild": "npm run eslint:check && npm run prettier:check && npm run stylelint:check",
    "build": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:update": "vitest run -u",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "cy:open": "cypress open",
    "cy:test": "start-server-and-test preview http://localhost:4173/pensjon/kalkulator 'ELECTRON_EXTRA_LAUNCH_ARGS=\"--force-prefers-reduced-motion\" cypress run'",
    "cy:test:production": "start-server-and-test preview http://localhost:4173/pensjon/kalkulator 'ELECTRON_EXTRA_LAUNCH_ARGS=\"--force-prefers-reduced-motion\" cypress run --env DECORATOR_URL=https://www.nav.no/dekoratoren'",
    "eslint:check": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "stylelint:check": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "prettier:check": "prettier --check '**/*.{cjs,js,jsx,ts,tsx,json}'",
    "prettier:fix": "prettier --write '**/*.{cjs,js,jsx,ts,tsx,json}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formatjs/intl-datetimeformat": "^6.12.0",
    "@formatjs/intl-numberformat": "^8.9.0",
    "@grafana/faro-web-sdk": "^1.2.9",
    "@grafana/faro-web-tracing": "^1.2.9",
    "@navikt/aksel-icons": "^5.11.3",
    "@navikt/ds-css": "^5.11.3",
    "@navikt/ds-react": "^5.11.3",
    "@navikt/ds-tokens": "^5.10.3",
    "@reduxjs/toolkit": "^1.9.7",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "highcharts": "^11.2.0",
    "highcharts-react-official": "^3.2.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@navikt/nav-dekoratoren-moduler": "^2.1.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/amplitude-js": "^8.16.5",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "amplitude-js": "^8.21.9",
    "axe-core": "^4.6.3",
    "cypress": "^13.6.0",
    "cypress-axe": "^1.4.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-typescript": "^1.0.10",
    "eslint-plugin-import": "^2.29.0",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.0",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.0.2",
    "msw": "^2.0.9",
    "node-sass": "^9.0.0",
    "openapi-typescript": "^7.0.0-next.4",
    "postcss": "^8.4.31",
    "postcss-modules-extract-imports": "^3.0.0",
    "postcss-modules-local-by-default": "^4.0.3",
    "postcss-modules-scope": "^3.0.0",
    "postcss-modules-values": "^4.0.0",
    "prettier": "^3.0.3",
    "react-intl": "6.5.5",
    "rollup-plugin-visualizer": "^5.9.3",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.2",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "timers": "^0.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-sass-dts": "^1.3.16",
    "vite-plugin-stylelint": "^5.2.1",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "**/*.{css,scss}": [
      "npm run stylelint:check"
    ],
    "**/*.{cjs,js,jsx,ts,tsx}": [
      "npm run prettier:check",
      "npm run eslint:check"
    ]
  }
}
